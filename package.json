{
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "author": "azu",
  "license": "MIT",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "name": "almin-usecase-map-generator",
  "version": "1.0.3",
  "description": "Command line tool that generate UseCase map from almin's UseCases.",
  "main": "lib/almin-usecase-map-generator.js",
  "bin": {
    "almin-usecase-map-generator": "./bin/cmd.js"
  },
  "scripts": {
    "test": "mocha \"test/**/*.ts\"",
    "build": "cross-env NODE_ENV=production tsc -p .",
    "watch": "tsc -p . --watch",
    "prepublish": "npm run --if-present build"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write --print-width 100 --tab-width 4",
      "git add"
    ]
  },
  "keywords": [
    "usecase",
    "almin",
    "figure"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/almin/almin-usecase-map-generator.git"
  },
  "bugs": {
    "url": "https://github.com/almin/almin-usecase-map-generator/issues"
  },
  "homepage": "https://github.com/almin/almin-usecase-map-generator",
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.37",
    "almin": "^0.18.1",
    "cross-env": "^5.2.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "mocha": "^6.2.3",
    "prettier": "^1.18.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@types/lodash.groupby": "^4.6.6",
    "@types/meow": "^5.0.0",
    "globby": "^10.0.2",
    "lodash.groupby": "^4.6.0",
    "meow": "^6.1.1",
    "nomnoml": "^0.7.1"
  },
  "husky": {
    "hooks": {
      "post-commit": "git reset",
      "pre-commit": "lint-staged"
    }
  }
}
